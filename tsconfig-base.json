{
  "compilerOptions": {
    "checkJs": true,
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noPropertyAccessFromIndexSignature": true,
    "moduleResolution": "node",
    "baseUrl": "src_ts"
  },
  "include": [
    "src_ts/*.ts"
  ]
}